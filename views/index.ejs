    <script>


document.addEventListener("DOMContentLoaded", function () {
            // Simulate a 5-second loading time
            setTimeout(function () {
                // Add the zoom-out class after 5 seconds
                const petLoader = document.querySelector(".pet-loader");
                petLoader.classList.add("zoom-out");

                // Hide the preloader after the zoom-out animation completes
                petLoader.addEventListener("animationend", function () {
                    const preloader = document.querySelector(".preloader");
                    preloader.style.display = "none";
                }, { once: true }); // Use { once: true } to ensure the event listener runs only once
            }, 5000); // 5000 milliseconds = 5 seconds
        });
        
        $(document).ready(function () {
            let isSubmitting = false; // Flag to track form submission status
    
            // Listen for the form submission
            $('#contact-form').submit(function (event) {
                event.preventDefault(); // Prevent the default form submission
    
                if (isSubmitting) {
                    return; // Do nothing if the form is already being submitted
                }
    
                // Disable the submit button
                $('#submit-button').prop('disabled', true);
    
                // Serialize the form data into a JSON object
                const formData = $(this).serializeArray();
                const data = {};
                formData.forEach(item => {
                    data[item.name] = item.value;
                });
    
                // Send the AJAX POST request
                $.ajax({
                    
                    success: function (response) {
                        console.log('Contact submitted successfully');
    
                        // Display the success message
                        $('#success').text('Message sent!').addClass('alert alert-success').fadeIn();
    
                        // Re-enable the submit button after a successful submission
                        $('#submit-button').prop('disabled', false);
    
                        // Set the new location after successful submission
                        window.location.href = "/"; // Replace "/" with the URL of your main screen
                    },
                    error: function (error) {
                        console.error('Error submitting contact:', error);
                        // Handle error (e.g., display an error message)
    
                        // Re-enable the submit button after an error
                        $('#submit-button').prop('disabled', false);
                    },
                    complete: function () {
                        isSubmitting = false; // Reset the submission flag
                    }
                });
    
                isSubmitting = true; // Set the submission flag
            });
        });

        
    </script>
    
    

</body>

</html>