        .letter {
            animation: bounce 5s ease-in-out infinite; /* Bounce animation */
            margin: 0 5px; /* Adjust the space between letters */
        }

        .zoom-out {
            animation: zoomOut 1s ease-in-out forwards; /* Zoom out animation after 5 seconds */
        }

        @keyframes bounce {
            0%, 20%, 50%, 80%, 100% {
                transform: translateY(0);
            }
            40% {
                transform: translateY(-20px);
            }
            60% {
                transform: translateY(-10px);
            }
        }

        @keyframes zoomOut {
            to {
                transform: scale(2); /* Adjust the zoom level */
            }
        }

        

